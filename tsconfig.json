{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./src",
      "paths": {
        "*": ["node_modules/*", "src/types/*"] 
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  